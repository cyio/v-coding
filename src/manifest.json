{
  "manifest_version": 2,
  "icons": { "128": "images/icon.png" },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [{
    "matches": [ "http://*/*", "https://*/*" ],
    "js": [ "js/content.js" ]
  }],
  "permissions": [
    "notifications",
    "cookies",
		"storage",
		"alarms",
		"tabs",
		"https://coding.net/*/*"
  ],
  "web_accessible_resources": [ "js/*", "html/*", "css/*", "images/*" ],
  "name": "<%= package.name %>",
  "version": "<%= package.version %>",
  "description": "<%= package.description %>"
}
