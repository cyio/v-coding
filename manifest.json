{
    "name": "v-coding",
    "version": "1.1.0",
    "manifest_version": 2,
    "description": "增强 coding.net 使用体验，源代码地址：https://github.com/cyio/v-coding",
    "author": "Oaker",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "scripts": [
            "libs/zepto.min.js",
            "libs/api.js",
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "v-coding",
        "default_popup": "popup.html"
    },
    "permissions": [
        "alarms",
        "tabs",
        "https://coding.net/*/*"
    ]
}
